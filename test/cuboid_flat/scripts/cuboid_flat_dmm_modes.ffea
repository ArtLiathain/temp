<param>
	<restart = 0>
	<dt = 1.00e-12>
	<kT = 4.11e-21>
	<check = 10000>
	<num_steps = 1e8>
	<rng_seed = time>
	<trajectory_out_fname = ../traj/cuboid_flat_dmm_trajectory_ffeadmm_blob0mode0.out>
	<measurement_out_fname = ../meas/cuboid_flat_dmm_measurement.out>
	<vdw_forcefield_params = ../structure/cuboid_flat.lj>
	<epsilon = 1.00e-02>
	<max_iterations_cg = 1000>
	<kappa = 1.00e+09>
	<epsilon_0 = 1.00e+00>
	<dielec_ext = 1.00e+00>
	<calc_stokes = 1>
	<stokes_visc = 1.00e-05>
	<calc_vdw = 0>
	<calc_noise = 1>
	<calc_es = 0>
	<es_update = 1>
	<es_N_x = 0>
	<es_N_y = 0>
	<es_N_z = 0>
	<sticky_wall_xz = 0>
	<wall_x_1 = PBC>
	<wall_x_2 = PBC>
	<wall_y_1 = PBC>
	<wall_y_2 = PBC>
	<wall_z_1 = PBC>
	<wall_z_2 = PBC>
	<es_h = 3>
	<num_blobs = 1>
	<num_conformations = (1)>
	<num_states = (1)>
</param>

<system>
	<blob>
		<conformation>
			<motion_state = DYNAMIC>
			<topology = ../structure/cuboid_flat.top>
			<material = ../structure/cuboid_flat.mat>
			<stokes = ../structure/cuboid_flat.stokes>
			<pin = ../structure/cuboid_flat.pin>
			<nodes = ../structure/cuboid_flat.node>
			<surface = ../structure/cuboid_flat.surf>
			<vdw = ../structure/cuboid_flat.vdw>
		</conformation>
		<solver = CG_nomass>
		<scale = 5e-9>
		<centroid = ( 0.00, 0.00, 0.00)>
	</blob>
</system>
