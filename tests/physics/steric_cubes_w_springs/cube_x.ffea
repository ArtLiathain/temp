<param>
	<restart = 0>
	<dt = 2.00e-15>
	<kT = 4.11e-21>
	<check = 100>
	<num_steps = 1e3>
	<rng_seed = time>
	<trajectory_out_fname = cube_x_trajectory.ftj>
	<measurement_out_fname = cube_x_measurement.fm>
	<vdw_forcefield_params = ../cube_springs_structure/cube_x.lj>
	<epsilon = 1.00e-02>
	<max_iterations_cg = 1000>
	<kappa = 1.00e+09>
	<epsilon_0 = 1.00e+00>
	<dielec_ext = 1.00e+00>
	<calc_stokes = 1>
	<stokes_visc = 1.00e-03>
	<calc_vdw = 1>
	<inc_self_vdw = 0>
	<vdw_type = steric>
	<vdw_steric_factor = 4.e1>
	<calc_springs=1>
	<calc_noise = 0>
	<calc_es = 0>
	<es_update = 1>
	<es_N_x = -1>
	<es_N_y = -1>
	<es_N_z = -1>
	<move_into_box = 1>
	<sticky_wall_xz = 0>
	<wall_x_1 = PBC>
	<wall_x_2 = PBC>
	<wall_y_1 = PBC>
	<wall_y_2 = PBC>
	<wall_z_1 = PBC>
	<wall_z_2 = PBC>
	<es_h = 3>
	<num_blobs = 2>
	<num_conformations = (1,1)>
</param>

<system>
	<blob>
		<conformation>
			<motion_state = DYNAMIC>
			<topology = ../cube_springs_structure/cube_x.top>
			<material = ../cube_springs_structure/cube_x.mat>
			<stokes = ../cube_springs_structure/cube_x.stokes>
			<pin = ../cube_springs_structure/cube_x.pin>
			<nodes = ../cube_springs_structure/cube_x.node>
			<surface = ../cube_springs_structure/cube_x.surf>
			<vdw = ../cube_springs_structure/cube_x.vdw>
		</conformation>
		<solver = CG_nomass>
		<scale = 2.00e-9>
		<centroid_pos = (0.,0.,0.)>
	</blob>
	<blob>
		<conformation>
			<motion_state = DYNAMIC>
			<topology = ../cube_springs_structure/cube_x.top>
			<material = ../cube_springs_structure/cube_x.mat>
			<stokes = ../cube_springs_structure/cube_x.stokes>
			<pin = ../cube_springs_structure/cube_x.pin>
			<nodes = ../cube_springs_structure/cube_x.node>
			<surface = ../cube_springs_structure/cube_x.surf>
			<vdw = ../cube_springs_structure/cube_x.vdw>
		</conformation>
		<solver = CG_nomass>
		<scale = 2.00e-9>
		<centroid_pos = (1.001,0.,0.)>
      <rotation = (-1.,0.,0.,0.,1.,0.,0.,0.,-1.)>
	</blob>
	<interactions>
		<springs>
			<springs_fname = ../cube_springs_structure/cube_xx.springs>
		</springs>
	</interactions>

</system>
