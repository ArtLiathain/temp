<param>
	<restart = 0>
	<dt = 1e-15>
	<kT = 4.11e-21>
	<check = 100>
	<num_steps = 4e6>
	<rng_seed = time>
	<trajectory_out_fname = traj/single_element_trajectory.out>
	<measurement_out_fname = meas/single_element_measurement.out>
	<vdw_forcefield_params = structure/single_element.lj>
	<epsilon = 0.01>
	<max_iterations_cg = 1000>
	<kappa = 1e9>
	<epsilon_0 = 1>
	<dielec_ext = 1>
	<calc_stokes = 1>
	<stokes_visc = 1e-05>
	<calc_vdw = 0>
	<calc_noise = 1>
	<calc_kinetics = 0>
	<kinetics_update = 10000>
	<calc_es = 0>
	<es_update = 1>
	<es_N_x = 0>
	<es_N_y = 0>
	<es_N_z = 0>
	<sticky_wall_xz = 0>
	<wall_x_1 = PBC>
	<wall_x_2 = PBC>
	<wall_y_1 = PBC>
	<wall_y_2 = PBC>
	<wall_z_1 = PBC>
	<wall_z_2 = PBC>
	<es_h = 3>
	<num_blobs = 1>
	<num_conformations = (1)>
	<num_states = (1)>
</param>

<system>
	<blob>
		<conformation>
			<motion_state = DYNAMIC>
			<nodes = structure/single_element.node>
			<topology = structure/single_element.top>
			<surface = structure/single_element.surf>
			<material = structure/single_element.mat>
			<stokes = structure/single_element.stokes>
			<vdw = structure/single_element.vdw>
			<pin = structure/single_element.pin>
		</conformation>
		<solver = CG>
		<scale = 1e-9>
		<centroid_pos = (0.0,0.0,0.0)>
	</blob>
</system>
