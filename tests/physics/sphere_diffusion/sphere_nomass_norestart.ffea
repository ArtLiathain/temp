<param>
	<restart = 0>
	<dt = 1.00e-14>
	<kT = 4.11e-21>
	<check = 100>
	<num_steps = 1e6>
	<rng_seed = time>
	<trajectory_out_fname = traj/sphere_nomass_norestart_trajectory.ftj>
	<measurement_out_fname = meas/sphere_nomass_norestart_measurement.fm>
	<vdw_forcefield_params = structure/sphere.lj>
	<checkpoint_out = sphere_nomass_norestart_out.fcp>
	<checkpoint_in = sphere_nomass_norestart_in.fcp>
	<epsilon = 1.00e-02>
	<max_iterations_cg = 1000>
	<kappa = 1.00e+09>
	<epsilon_0 = 1.00e+00>
	<dielec_ext = 1.00e+00>
	<calc_stokes = 1>
	<stokes_visc = 1.00e-03>
	<calc_vdw = 1>
	<calc_noise = 1>
	<calc_es = 0>
	<es_update = 1>
	<es_N_x = 10>
	<es_N_y = 10>
	<es_N_z = 10>
	<move_into_box = 1>
	<sticky_wall_xz = 0>
	<wall_x_1 = PBC>
	<wall_x_2 = PBC>
	<wall_y_1 = PBC>
	<wall_y_2 = PBC>
	<wall_z_1 = PBC>
	<wall_z_2 = PBC>
	<es_h = 3>
	<num_blobs = 1>
	<num_conformations = (1)>
</param>

<system>
	<blob>
		<conformation>
			<motion_state = DYNAMIC>
			<topology = structure/sphere.top>
			<material = structure/sphere.mat>
			<stokes = structure/sphere.stokes>
			<pin = structure/sphere.pin>
			<nodes = structure/sphere.node>
			<surface = structure/sphere.surf>
			<vdw = structure/sphere.vdw>
		</conformation>
		<solver = CG_nomass>
		<scale = 5.00e-09>
		<centroid = ( 0.00, 0.00, 0.00)>
	</blob>
</system>
