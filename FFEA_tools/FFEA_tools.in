#!/bin/sh

#################################################

# NORMALLY, YOU DON'T NEED TO MODIFY BELOW THIS LINE

#################################################

# Assume we were started as FFEA/bin/ffea_tools and
# deduce what FFEA should be.  Try to work when
# pydock is linked to with a symbolic link

tmp=`ls -l $0`
FFEA=`echo "$tmp" | sed 's/.* -> //'`
if test "$tmp" = "$FFEA"
then
      FFEA="$0"
fi
FFEA=`dirname "$FFEA"`
FFEA=`cd "$FFEA"; pwd -P`
FFEA=`dirname "$FFEA"`
export FFEA

PYTHONFOLDER=$FFEA/lib/python@PYTHON_VERSION_MAJOR@.@PYTHON_VERSION_MINOR@/
# export PYTHONPATH=$PYTHONPATH:$FFEA/lib/python@PYTHON_VERSION_MAJOR@.@PYTHON_VERSION_MINOR@/
export PYTHONPATH=$PYTHONFOLDER/FFEA_initialise/FFEA_python_modules:$PYTHONFOLDER/FFEA_analysis/FFEA_python_modules:$PYTHONPATH


exec python $FFEA/lib/python@PYTHON_VERSION_MAJOR@.@PYTHON_VERSION_MINOR@/FFEA_tools.py $@



